{
  "version": 2,
  "builds": [
    {
      "src": "snapfeast/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "100mb", "runtime": "python3.11" }
    },
    {
      "src": "build.sh",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "1000mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "snapfeast/wsgi.py"
    }
  ]
}